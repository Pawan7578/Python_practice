    "class Calc:\n",
    "    def add(self, *args):\n",
    "        return sum(args)\n",
    "\n",
    "def main():\n",
    "    result = Calc()\n",
    "    print(result.add(5))\n",
    "    print(result.add(5,10))\n",
    "    print(result.add(5,10,15))\n",
    "    print(result.add(5,10,15,20))\n",
    "main()"
